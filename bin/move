#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
puts "Where would you like to go?"
input = gets.strip
input_to_index(input)
puts "Would you like to be X or O"
player = gets.strip
move(board, input, player)
